        <errorConditionFormula>IF(
      (
            (FKClient__c != NULL  &amp;&amp;  FKLead__c == NULL &amp;&amp; FKContact__c == NULL) ||
            (FKClient__c == NULL  &amp;&amp;  FKLead__c != NULL &amp;&amp; FKContact__c == NULL) ||
            (FKClient__c == NULL  &amp;&amp;  FKLead__c == NULL &amp;&amp; FKContact__c != NULL) 
