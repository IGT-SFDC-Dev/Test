    <validationRules>
        <fullName>UniqueActionIDCheck</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISNEW(),
NOT(ISBLANK(VLOOKUP( $ObjectType.Action__c.Fields.Name , $ObjectType.Action__c.Fields.Name,NULLVALUE(Name, Name)))),
IF(ISCHANGED(Name),
IF((LOWER(Name) = LOWER(VLOOKUP($ObjectType.Action__c.Fields.Name , $ObjectType.Action__c.Fields.Name,Name))&amp;&amp;LOWER(PRIORVALUE(Name)) != Lower(Name)), TRUE, FALSE),
FALSE))</errorConditionFormula>
        <errorMessage>An action record with this Action already exists. Enter a unique Action.</errorMessage>
    </validationRules>
