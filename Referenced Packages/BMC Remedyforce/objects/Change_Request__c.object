        <description>This rule checks to make sure that Actual End Date comes always after Actual Start Date.</description>
        <errorConditionFormula>AND(NOT(OR(ISNULL(Actual_End_Date__c),ISBLANK(Actual_End_Date__c) ) ),OR( Actual_End_Date__c &lt; Actual_Start_Date__c,  Actual_End_Date__c = Actual_Start_Date__c) )</errorConditionFormula>
        <errorMessage>Actual End Date must be later than Actual Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_ReviewDate</fullName>
        <active>true</active>
        <description>This rule checks to make sure that Review Date comes always after Actual End Date.</description>
        <errorConditionFormula>AND(NOT(OR(ISNULL(Review_Date__c),ISBLANK(Review_Date__c) ) ),OR( Review_Date__c &lt; Actual_End_Date__c,  Actual_End_Date__c = Review_Date__c) )</errorConditionFormula>
        <errorMessage>The Review Date must be later than the Actual End Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_ScheduledEndDate</fullName>
        <active>true</active>
