            <picklistValues>
                <fullName>Standard Expedited</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Normal Expedited</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Emergency</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
