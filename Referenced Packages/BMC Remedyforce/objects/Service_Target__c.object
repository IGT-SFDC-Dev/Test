        <fullName>TargetTypeValue__c</fullName>
        <deprecated>false</deprecated>
        <description>This field derives the value from custom label based on value in Target Type field.</description>
        <externalId>false</externalId>
        <formula>IF(LOWER(Target_Type__c)==&apos;resolutiontime&apos;, $Label.bmcservicedesk__ResolutionTime,
IF(LOWER(Target_Type__c)==&apos;initiationtime&apos;, $Label.bmcservicedesk__InitiationTime,
IF(LOWER(Target_Type__c)==&apos;responsetime&apos;, $Label.bmcservicedesk__ResponseTime,
IF(LOWER(Target_Type__c)==&apos;completiontime&apos;, $Label.bmcservicedesk__CompletionTime,&apos;&apos;))))</formula>
