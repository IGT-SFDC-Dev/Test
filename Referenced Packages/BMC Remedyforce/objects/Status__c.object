IF((LOWER(Name) = LOWER(VLOOKUP($ObjectType.Status__c.Fields.Name , $ObjectType.Status__c.Fields.Name,Name))&amp;&amp;LOWER(PRIORVALUE(Name)) != Lower(Name)), TRUE, FALSE),
FALSE))</errorConditionFormula>
        <errorMessage>A record with this Status already exists. Enter unique Status.</errorMessage>
    </validationRules>
