    <validationRules>
        <fullName>ImpactId_required</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISNULL(Name ) , ISBLANK(Name))</errorConditionFormula>
        <errorMessage>Please enter Impact.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UniqueImpactIdCheck</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISNEW(),
NOT(ISBLANK(VLOOKUP( $ObjectType.Impact__c.Fields.Name , $ObjectType.Impact__c.Fields.Name,NULLVALUE(Name, Name)))),
IF(ISCHANGED(Name),
IF((LOWER(Name) = LOWER(VLOOKUP($ObjectType.Impact__c.Fields.Name , $ObjectType.Impact__c.Fields.Name,Name))&amp;&amp;LOWER(PRIORVALUE(Name)) != Lower(Name)), TRUE, FALSE),
FALSE))</errorConditionFormula>
        <errorMessage>An record with this Impact already exists. Enter unique Impact.</errorMessage>
    </validationRules>
