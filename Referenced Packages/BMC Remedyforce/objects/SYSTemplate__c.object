    <validationRules>
        <fullName>templateNameUniqueValidation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISNEW(),
NOT(ISBLANK(VLOOKUP( $ObjectType.SYSTemplate__c.Fields.Name , $ObjectType.SYSTemplate__c.Fields.Name,NULLVALUE(Name, Name) ))),
IF(ISCHANGED(Name),
IF(LOWER(Name) = LOWER(VLOOKUP($ObjectType.SYSTemplate__c.Fields.Name , $ObjectType.SYSTemplate__c.Fields.Name,Name)), TRUE, FALSE),
FALSE))</errorConditionFormula>
        <errorMessage>Duplicate Name</errorMessage>
