        function openLookup(){        	
        	if(popUpString.indexOf('FK') == 0){
        		popUpString = popUpString.substring(2,popUpString.length);
        	}     	
        	if(popUpString.endsWith('__c')){
        		popUpString = popUpString.substring(0,popUpString.length - 3);
        	}
        	if(popUpString == 'ACOpRuleID') {
        		if(isFPUserValidated){
        			return showORWindow();
        		}else{
        			openPopupWithTitle('ACFPUserCredentialPage',oncompleteFPvalidateJS,'{!JSENCODE($Label.ACFPlaunchWindowHeader)}',Ext.isIE?240:227,475);
					return popUpWindow.center();
        		}
        	}
        	if(popUpString == 'BMC BaseElement'){
        		popUpString  = 'BMC_BaseElement';
        	}else if(popUpString == 'ClosedBy' || popUpString == 'Closed_By' || popUpString == 'Initiator'){
        		popUpString = 'Client';
        	}
        	if( popUpString == 'CIsAcquiredBy' ||popUpString == 'ContractsVerifiedBy' || popUpString == 'LicensesVerifiedBy' || popUpString == 'ConnectionReceive'
        	||popUpString == 'ReleaseCoordinator' ||popUpString == 'TestingCompletedBy' ){
        		popUpString ='OwnerId';// to show staff users on selection of above field
        	}
            if(popUpString == 'Status'){
            	var templateFor='{!JSENCODE(template.templateFor__c)}';
            	if(templateFor == 'Task'){
            		openPopup('SearchPage?popupId='+popUpString+'&isLookup=true&filterClause='+escape("appliesToTask__c=true"), preSetReferenceToText);
            	}else if(templateFor == 'Incident'){
            		openPopup('SearchPage?popupId='+popUpString+'&isLookup=true&filterClause='+escape("appliesToIncident__c=true"), preSetReferenceToText);
            	}else if(templateFor == 'Release'){
            		openPopup('SearchPage?popupId='+popUpString+'&isLookup=true&filterClause='+escape("appliesToRelease__c=true"), preSetReferenceToText);
            	}else{
            		openPopup('SearchPage?popupId='+popUpString+'&isLookup=true&filterClause='+escape("appliesToChange__c=true"), preSetReferenceToText);
            	}
            }else if(popUpString == 'OpenBy' || popUpString == 'Staff'){
        		openPopup('SearchPage?popupId=Client&isLookup=true&filterClause=IsStaffUser__c=true',preSetReferenceToText);
            }else if(popUpString == 'BusinessService'){            	
                openPopup('SearchPage?popupId=BMC_BusinessService&isLookup=true&filterClause=' + dateClause, preSetReferenceToText);
            }else if(popUpString == 'ServiceOffering'){  //change 
                popUpString  = 'BMC_BusinessService';  	
               openPopup('SearchPage?popupId=BMC_BusinessService&isLookup=true&filterClause=' + escape('serviceType__c = \'Offering\' and ')+dateClause, preSetReferenceToText);//Change
            }else if(popUpString != 'OwnerId'){
                openPopup('SearchPage?popupId='+popUpString+'&isLookup=true', preSetReferenceToText);
            }else{
            	var templateFor='{!JSENCODE(template.templateFor__c)}';
                openPopup('SearchPage?popupId=Client&isQueurorUser=true&isLookup=true&queueFor='+templateFor+'&filterClause='+escape("IsStaffUser__c=true"), preSetReferenceToText);
            }                  
        }
		//Added for user lookup custom field        
        function openLookup1(){
        	popUpString = 'Staff';
        	openPopup('SearchPage?popupId=Client&isLookup=true&filterClause=IsStaffUser__c=true',preSetReferenceToText);
        }
                
